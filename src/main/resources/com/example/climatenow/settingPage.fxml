<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.climatenow.SettingPageController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="1000.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@settingBackgroundNew.png" />
         </image>
      </ImageView>
      <Button fx:id="changeUsernameButton" layoutX="170.0" layoutY="281.0" mnemonicParsing="false" onAction="#ClickChangeUsernameButton" opacity="0.0" prefHeight="76.0" prefWidth="662.0" style="-fx-background-color: #A482FF;">
         <font>
            <Font name="Futura Medium" size="13.0" />
         </font>
      </Button>
      <Button fx:id="changePasswordButton" layoutX="169.0" layoutY="370.0" mnemonicParsing="false" onAction="#ClickChangePasswordButton" opacity="0.0" prefHeight="76.0" prefWidth="662.0" style="-fx-background-color: #A482FF;">
         <font>
            <Font name="Futura Medium" size="13.0" />
         </font>
      </Button>
      <Button fx:id="logoutButton" layoutX="169.0" layoutY="460.0" mnemonicParsing="false" onAction="#JumpOutYesAndNoWindow" opacity="0.0" prefHeight="76.0" prefWidth="662.0" style="-fx-background-color: #A482FF;">
         <font>
            <Font name="Futura Medium" size="13.0" />
         </font>
      </Button>
      <Button fx:id="backToHomePageButton" layoutX="170.0" layoutY="193.0" mnemonicParsing="false" onAction="#switchToHomePage" opacity="0.0" prefHeight="76.0" prefWidth="662.0" style="-fx-background-color: #635bfd;" />
   </children>
</AnchorPane>
