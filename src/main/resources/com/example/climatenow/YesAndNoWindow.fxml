<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.climatenow.SettingPageController">
    <Button fx:id="yesButton" layoutX="35.0" layoutY="78.0" mnemonicParsing="false" onAction="#changeToLoginScene" prefHeight="45.0" prefWidth="80.0" style="-fx-background-color: #A482FF;" text="YES">
        <font>
            <Font name="Futura Medium" size="20.0" />
        </font>
    </Button>
   <Button fx:id="noButton" layoutX="185.0" layoutY="78.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="45.0" prefWidth="80.0" style="-fx-background-color: #A482FF;" text="NO">
      <font>
         <Font name="Futura Medium" size="20.0" />
      </font>
   </Button>
</AnchorPane>
