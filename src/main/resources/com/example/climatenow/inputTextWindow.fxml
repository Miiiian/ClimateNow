<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.climatenow.SettingPageController">
      <TextField fx:id="newInputTextField" layoutX="408.0" layoutY="279.0" prefHeight="35.0" prefWidth="228.0" />
      <Text fx:id="textOnInputWindow" layoutX="217.0" layoutY="305.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Please enter:" wrappingWidth="228.0">
         <font>
            <Font name="Futura Medium" size="30.0" />
         </font>
      </Text>
   <Button fx:id="yesButton" layoutX="407.0" layoutY="383.0" mnemonicParsing="false" onAction="#changePasswordOrUsernameFromTextField" prefHeight="45.0" prefWidth="80.0" style="-fx-background-color: #A482FF;" text="YES">
      <font>
         <Font name="Futura Medium" size="15.0" />
      </font>
   </Button>
   <Button fx:id="noButton" layoutX="554.0" layoutY="382.0" mnemonicParsing="false" onAction="#closeWindow" prefHeight="45.0" prefWidth="80.0" style="-fx-background-color: #A482FF;" text="NO">
      <font>
         <Font name="Futura Medium" size="15.0" />
      </font>
   </Button>
   <Button fx:id="showDialogButton" layoutX="108.0" layoutY="516.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="800.0" style="-fx-background-color: #FFAFEA;" textAlignment="CENTER" textFill="WHITE" visible="false">
      <font>
         <Font size="20.0" />
      </font>
   </Button>
</AnchorPane>
